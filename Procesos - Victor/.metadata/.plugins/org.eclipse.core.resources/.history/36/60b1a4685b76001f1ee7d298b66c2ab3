/**
 * 
 */
package paquete_inicial;


/**
 * 
 */
public class MiProceso extends Thread {

	@Override
	public void run() {
		// TODO Auto-generated method stub
		super.run();
		
		for (int i = 0; i < 100; i++) {
			System.out.println("HolaMundo desde Mi Proceso");
			try {
				Thread.sleep(1);
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
		}
		
	}

}
